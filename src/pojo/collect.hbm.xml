<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
 <hibernate-mapping package="pojo">
	<class name="collect" table="collect" >
		<id name="collect_id"  >
			<generator class="native"></generator>
		</id>
		<property name="use_id" column="use_id" ></property>
		<property name="talk_id" column="talk_id" ></property>
		<property name="talkcontent" column="talkcontent" ></property>
		<property name="talktime" column="talktime" ></property>
		<property name="talkphoto" column="talkphoto" ></property>
		<property name="nickName" column="nickName" ></property>
		<property name="avaPath" column="avaPath" ></property>
		<property name="school" column="school" ></property>
		<property name="collectUse_id" column="collectUse_id" ></property>
	</class>
 </hibernate-mapping>