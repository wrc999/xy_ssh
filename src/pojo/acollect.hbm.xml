<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
 <hibernate-mapping package="pojo">
	<class name="acollect" table="acollect" >
		<id name="acollect_id"  >
			<generator class="native"></generator>
		</id>
		<property name="use_id" column="use_id" ></property>
		<property name="acollecttime" column="acollecttime" ></property>
		<property name="activity_id" column="activity_id" ></property>
		<property name="ause_id" column="ause_id" ></property>
		<property name="joins" column="joins" ></property>
		<property name="name" column="name" ></property>
		<property name="starttime" column="starttime" ></property>
		<property name="endtime" column="endtime" ></property>
		<property name="place" column="place" ></property>
		<property name="photo" column="photo" ></property>
		<property name="intro" column="intro" ></property>
		<property name="school" column="school" ></property>
		<property name="nickName" column="nickName" ></property>
	</class>
 </hibernate-mapping>